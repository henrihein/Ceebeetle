        <Label Content="Receiving a file from {0}" Height="27" Margin="12,2,76,0" Name="lSender" VerticalAlignment="Top" />
        <Label Content="" Height="29" Margin="12,38,15,0" Name="lFilename" VerticalAlignment="Top" />
        <Label Content="Save to:" Height="25" HorizontalAlignment="Left" Margin="12,90,0,0" Name="label3" VerticalAlignment="Top" Width="68" />
        <TextBox Height="25" Margin="77,92,91,0" Name="tbFilename" VerticalAlignment="Top" TextChanged="tbFilename_TextChanged" />
        <Button Content="_Browse" Height="26" HorizontalAlignment="Right" Margin="0,92,10,0" Name="btnBrowse" VerticalAlignment="Top" Width="75" Click="btnBrowse_Click" />
