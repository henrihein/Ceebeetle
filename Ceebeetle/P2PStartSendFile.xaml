<src:CCBChildWindow x:Class="Ceebeetle.P2PStartSendFile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:src="clr-namespace:Ceebeetle"
        Title="Send File" Height="333" Width="300">
    <Grid>
        <ListBox Height="191" HorizontalAlignment="Left" Margin="12,36,0,0" Name="lbUsers" VerticalAlignment="Top" Width="139" />
        <Label Content="Send to:" Height="26" HorizontalAlignment="Left" Margin="12,2,0,0" Name="label1" VerticalAlignment="Top" Width="129" />
        <Button Content="_Send now" Height="24" HorizontalAlignment="Right" Margin="0,36,12,0" Name="btnSend" VerticalAlignment="Top" Width="99" Click="btnSend_Click" />
        <Button Content="_Cancel" Height="24" HorizontalAlignment="Right" Margin="0,65,12,0" Name="btnCancel" VerticalAlignment="Top" Width="99" Click="btnCancel_Click" />
        <Label Content="File to send:" Height="26" HorizontalAlignment="Left" Margin="14,232,0,0" Name="label2" VerticalAlignment="Top" Width="92" />
        <TextBox Height="24" Margin="10,0,0,9" Name="tbFile" VerticalAlignment="Bottom" TextChanged="tbFile_TextChanged" HorizontalAlignment="Left" Width="180" />
        <Button Content="_Browse..." Height="24" HorizontalAlignment="Right" Margin="0,0,12,9" Name="btnBrowse" VerticalAlignment="Bottom" Width="70" Click="btnBrowse_Click" />
    </Grid>
</src:CCBChildWindow>
