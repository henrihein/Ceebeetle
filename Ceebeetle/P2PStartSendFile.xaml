<src:CCBChildWindow x:Class="Ceebeetle.P2PStartSendFile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:src="clr-namespace:Ceebeetle"
        Title="Send File" Height="333" Width="300">
    <Grid>
        <ListBox Margin="12,86,127,15" Name="lbUsers" SelectionChanged="lbUsers_SelectionChanged" />
        <Label Content="Send to:" Height="26" HorizontalAlignment="Left" Margin="12,52,0,0" Name="label1" VerticalAlignment="Top" Width="139" />
        <Button Content="_Send now" Height="24" HorizontalAlignment="Right" Margin="0,0,12,45" Name="btnSend" VerticalAlignment="Bottom" Width="99" Click="btnSend_Click" />
        <Button Content="_Cancel" Height="24" HorizontalAlignment="Right" Margin="0,0,12,15" Name="btnCancel" VerticalAlignment="Bottom" Width="99" Click="btnCancel_Click" />
        <Label Content="File to send:" Height="26" HorizontalAlignment="Left" Margin="8,0,0,0" Name="label2" VerticalAlignment="Top" Width="96" />
        <TextBox Height="24" Margin="8,0,90,241" Name="tbFile" VerticalAlignment="Bottom" TextChanged="tbFile_TextChanged" />
        <Button Content="_Browse..." Height="24" HorizontalAlignment="Right" Margin="0,29,12,0" Name="btnBrowse" VerticalAlignment="Top" Width="70" Click="btnBrowse_Click" />
    </Grid>
</src:CCBChildWindow>
