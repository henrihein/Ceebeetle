<src:CCBChildWindow x:Class="Ceebeetle.BagSelector"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:src="clr-namespace:Ceebeetle"
        Title="Select Bag" Height="317" Width="245">
    <Grid>
        <ListBox Name="lbBags" Margin="12,0,12,84" SelectionChanged="lbBags_SelectionChanged" />
        <Button Content="_Select" Height="24" Margin="12,0,12,46" Name="btnSelect" VerticalAlignment="Bottom" Click="btnSelect_Click" />
        <Button Content="Close" Height="24" Margin="12,0,12,12" Name="btnClose" VerticalAlignment="Bottom" Click="btnClose_Click" />
    </Grid>
</src:CCBChildWindow>
